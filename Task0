import csv

with open('texts.csv', 'r') as f:
    reader = csv.reader(f)
    texts = list(reader)

with open('calls.csv', 'r') as f:
    reader = csv.reader(f)
    calls = list(reader)

telemarketers_list = []
for number in calls:
	if number[0].startswith("140"):
		telemarketers_list.append(number[0])

telemarketers = []
for n in telemarketers_list:
	if n not in telemarketers:
		telemarketers.append(n)

print(telemarketers)		
